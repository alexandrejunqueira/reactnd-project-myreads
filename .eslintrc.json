{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jasmine": true
  },
  "parser": "babel-eslint",
  "rules": {
    "max-len": "off",
    "react/no-unescaped-entities": "off",
    "react/prefer-stateless-function": "off",
    "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
    "jsx-a11y/no-static-element-interactions": "off"
  }
}
